@import "../demo/demo.wxss";
/* pages/minepersonhomepage/minepersonhomepage.wxss */
page{
  background:#f8f8f8;
}
.wrapminepage
{

}
.minepageheader
{
  width:100%;
  height: 600rpx;
  background:#f8f8f8;
  position:relative;
}
.headimg image{
  width:100%;
  height:521rpx;
}
.wraploginAndEaring
{
  width:92%;
  height:228px;
  background:#fff;
  opacity:0.86;
  position:absolute;
  top:150rpx;
  border-radius:8rpx;
  left:4%;
  }
 
  .bigCircle image
  {
    width:60rpx;
    height:60rpx;
    border-radius:50%;
  }
  .bigCircle  text
  {
    font-size:26rpx;
    color:#fff;
    margin-top:20rpx;
  }
  .secondpart
  {
    width:100%;
    height:190rpx;
    background:#f8f8f8;
    display:flex;
    flex-direction:row;
    
    justify-content:space-between;
    align-items:center;
  }
  .clickToLogin
  {
    font-size:30rpx;
    width:100%;
    text-align:center;
    margin-top:90rpx;
  }
  .secondpartItem
  {
    flex:1;
    display:flex;
    flex-direction:column;
    text-align:center;
    justify-content:center;
    height:190rpx;
    font-size:30rpx;
    align-items:center;
    position:relative;
  }
  .secondpartItemImg
  {
    width:42rpx;
    height:42rpx;
  }
  .secondpartItemText
  {
    margin-top:15rpx;
  }
  .bigCircle
  {
    width:142rpx;
    height:142rpx;
    background:#5CDAD9;
    border:3px solid #fff;
    position:absolute;
    top:-71rpx;
    left:50%;
    display:flex;
    align-items:center;
    flex-direction:column;
    justify-content:center;
    margin-left:-71rpx;
    border-radius:50%;
  }
  .moneypart
  {
    margin-left:60rpx;
    margin-right:60rpx;
    height:110rpx;
    margin-top:64rpx;
    display:flex;
    flex-direction:row;
  }
  .moneypartLeft
  {
    flex:1;
    height:110rpx;
     display:flex;
    flex-direction:column;
    align-items:center;
    justify-content:center;
  }
  .moneypartRight
  {
    flex:1;
    height:110rpx;
    display:flex;
    flex-direction:column;
    align-items:center;
    justify-content:center;
  }
  .moneypartLeft1-1,.moneypartRight1-1
  {
    font-size:24rpx;
    color:#5CDAD9;
  }
  .moneypartLeft1-2,.moneypartRight1-2
  {
    font-size:54rpx;
    color:#5CDAD9;
    margin-left:5rpx;
  }
  .moneypartLeft2,.moneypartRight2
  {
    font-size:26rpx;
    color:#5CDAD9;
  }
  .cardAndMember
  {
    margin-left:60rpx;
    margin-right:60rpx;
    height:96rpx;
    display:flex;
    flex-direction:row;
    align-items:center;
    justify-content:space-between;
    margin-top:26rpx;
  }
  .cardAndMemberLeft,.cardAndMemberRight
  {
    display:flex;
    flex-direction:column;
    /* flex:1; */
    width:46%;
    background:#e5fafb;
    height:96rpx;
    align-items:center;
    text-align:center;
    justify-content:center;
  }
  .addmoneypartLeft1-2,.addmoneypartRight1-2
  {
    font-size:40rpx;
    color:#333;
  }
  .addmoneypartLeft2,.addmoneypartRight2
  {
  color:rgba(0,0,0,0.36);
  font-size:24rpx;
  margin-top:-6rpx!important;
  }
  .addmoneypartLeft1-1
  {
    color:#000;
  }
  .wrapRecommendCtn
  {
      width:92%;
      height:auto;
      padding-bottom:40rpx;
      margin-left:4%;
  }
  .recommendCtnTitle
  {
      width:98%;
      height:74rpx;
      background: #fff;
      border-radius:5rpx;
      font-size:26rpx;
      color:rgba(0,0,0,0.66);
      margin-left:1%;
      letter-spacing:2rpx;
      line-height:74rpx;
      text-align:center;
  }
  .numberOfOrder
  {
    padding:3rpx 24rpx;
    background:#fe6665;
    font-size:10px;
    color:#fff;
    border-top-left-radius:8px;
    border-bottom-left-radius:0px;
    border-top-right-radius:8px;
    border-bottom-right-radius:8px;
    position:absolute;
    right:8px;
    bottom:60px;
  }
